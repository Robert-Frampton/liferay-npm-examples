{
	"exclude": {
		"vue@2.3.4": ["src/**/*.js"]
	},
	"*": {
		".babelrc": {
			"plugins": [
				"normalize-requires",
				["shim-node-globals", {
					"globals": {
						"global": "var global = window;",
						"process": "var process = {env:{NODE_ENV: 'production'}};"
					}
				}],
				"wrap-modules-amd",
				["name-amd-modules", {
					"sourceRoot": "{{SRC_DIR}}"
				}],
				"namespace-amd-define"
			]
		}
	}
}
